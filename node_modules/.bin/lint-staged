#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/bin/lint-staged.js/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/bin/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/lint-staged@10.5.4/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/nxtwave/react-hooks/sessions/node_modules:/home/nxtwave/react-hooks/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/lint-staged/bin/lint-staged.js/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/lint-staged/bin/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/lint-staged/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else 
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
