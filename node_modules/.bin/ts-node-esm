#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/ts-node@10.7.0/node_modules/ts-node/dist/bin-esm.js/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/ts-node@10.7.0/node_modules/ts-node/dist/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/ts-node@10.7.0/node_modules/ts-node/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/ts-node@10.7.0/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/nxtwave/react-hooks/sessions/node_modules:/home/nxtwave/react-hooks/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/ts-node/dist/bin-esm.js/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/ts-node/dist/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-esm.js" "$@"
else 
  exec node  "$basedir/../ts-node/dist/bin-esm.js" "$@"
fi
