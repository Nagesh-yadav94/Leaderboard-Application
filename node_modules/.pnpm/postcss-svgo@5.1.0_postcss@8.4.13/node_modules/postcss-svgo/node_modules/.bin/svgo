#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/svgo/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/svgo@2.8.0/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/nxtwave/react-hooks/sessions/node_modules:/home/nxtwave/react-hooks/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/postcss-svgo@5.1.0_postcss@8.4.13/node_modules/postcss-svgo/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/postcss-svgo@5.1.0_postcss@8.4.13/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
else 
  exec node  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
fi
