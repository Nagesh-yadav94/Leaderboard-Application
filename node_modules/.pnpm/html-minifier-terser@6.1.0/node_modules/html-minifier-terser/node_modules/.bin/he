#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/he/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/he@1.2.0/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/nxtwave/react-hooks/sessions/node_modules:/home/nxtwave/react-hooks/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/html-minifier-terser@6.1.0/node_modules/html-minifier-terser/node_modules:/home/nxtwave/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/html-minifier-terser@6.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else 
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
